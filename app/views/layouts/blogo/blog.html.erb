    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
</header>

  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

<%= render partial: "partials/join_us_overlay" %>
<%= render partial: "partials/contact_us_overlay" %>
  <div id="container" class="blog-container">
    <header class="blogo-header">
      <div class='header-titles'>
        <h1><%= link_to Blogo.config.site_title, blogo_root_path %></h1>
        <h2><%= Blogo.config.site_subtitle %></h2>
      </div>

      <div class='header-icons'>
        <%= render partial: 'blogo/shared/social_icons' %>
      </div>
    </header>

    <div id="content">
      <div class="pure-g-r">
        <div class="pure-u-3-4">
          <%= yield %>
        </div>

        <div class="pure-u-1-4">
